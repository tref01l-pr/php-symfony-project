{% extends 'base-templates/adminBase.html.twig' %}

{% block content %}

    <h1>All Users</h1>
    <ul>
        {% for user in users %}
            <li>{{ user.username }} ({{ user.email }})
                <button><a href="{{ path('app_admin_user_info', {'id': user.id}) }}">More Info</a></button>
            </li>
        {% endfor %}
    </ul>

{% endblock %}